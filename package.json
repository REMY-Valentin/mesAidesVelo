    "@sveltejs/adapter-vercel": "^3.0.1",
    "@sveltejs/kit": "^1.21.0",
    "mdsvex": "^0.11.0",
    "node-fetch": "^3.3.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "sharp": "=0.32.1",
    "svelte": "^4.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vite-plugin-windicss": "^1.9.0",
    "vitest": "^0.32.2",
    "wget-improved": "^3.4.0",
    "yaml": "^2.3.1"
  },
  "dependencies": {
    "@etalab/decoupage-administratif": "^3.0.0",
    "fuzzysort": "^2.0.4",
    "jsdom": "^22.1.0",
